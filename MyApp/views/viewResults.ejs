<html>
    <head>
        <% include partials/head.ejs %>
    </head>
    <style>
        body { padding-top: 60px; padding-bottom: 30px; }
    </style>
    <body>
        <%var entries = JSON.parse(responses)%>
        <div class="col-sm-1"></div>
        <div class="col-sm-10">
            <div class="container-fluid">
            <!--<div class="panel panel-default">-->
                <!--<div class="panel panel-heading">-->
            <table class="table table-striped table-condensed table-responsive table-hover table-bordered">

                    <thead>
                        <tr>
                            <th>Student's Name</th>
                            <th>Score Obtained</th>
                            <th>Total Score</th>
                        </tr>
                    </thead>
                <!--</div>-->
                <%entries.forEach((entry)=>{%>
                <!--<div class="panel panel-body">-->
                    <tbody>
                        <tr>
                            <td><%=entry.fullname%></td>
                            <td><%=entry.marks_obtd%></td>
                            <td><%=entry.total_marks%></td>
                        </tr>
                    </tbody>
                <!--</div>-->
                <%})%>
            </div>
            </table>
            <!--</div>-->
        </div>
        <div class="col-sm-1"></div>
    </body>
</html>